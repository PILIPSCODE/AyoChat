{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/tampilan"
      }
    ],
    "redirects": [
    { "source": "/tampilan", "destination": "/index.html", "permanent": false }
  ]
  }
